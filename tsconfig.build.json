{
	"extends": "./tsconfig.json",
	"compilerOptions": {
		"target": "ES2020",
		"module": "ESNext",
		"moduleResolution": "Bundler",
		
		"jsx": "react-jsx",

		"skipLibCheck": true,

		"declaration": true,
		"emitDeclarationOnly": false,
		"declarationMap": true,
		"noEmit": false
	},
	"include": ["src"]
}
